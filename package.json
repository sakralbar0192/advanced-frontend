  "scripts": {
    "start": "webpack serve --env port=3000 --env mode=development",
    "start:dev:server": "node json-server/index.js",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "test:unit": "jest --config=config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "concurrently -s first -k \"npm run storybook\" \"npm run test:ui\" \"deelay\"",
    "lint:ts": "eslint \"**/*.{ts, tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts, tsx}\" --fix",
    "lint:styles": "stylelint \"**/*.{scss, css}\"",
    "lint:styles:fix": "eslint \"**/*.{scss, css}\" --fix",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybook",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:ui:report:json": "node .loki/scripts/generate-visual-json-report.js",
    "test:ui:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky"
  },
